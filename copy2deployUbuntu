DEPLOY_FOLDER="../../Deploys/Ubuntu/DIS"
rm -r -f $DEPLOY_FOLDER/*
cp ./*/bin/* $DEPLOY_FOLDER/
cp ./*/*.properties $DEPLOY_FOLDER/

JAVA_SERVICE="DISAgentManagementService"
mkdir $DEPLOY_FOLDER/$JAVA_SERVICE
mkdir $DEPLOY_FOLDER/$JAVA_SERVICE/tmp
mkdir $DEPLOY_FOLDER/$JAVA_SERVICE/log
cp -r ./$JAVA_SERVICE/cmd $DEPLOY_FOLDER/$JAVA_SERVICE
cp -r ./$JAVA_SERVICE/conf $DEPLOY_FOLDER/$JAVA_SERVICE
cp -r ./$JAVA_SERVICE/dist $DEPLOY_FOLDER/$JAVA_SERVICE
cp ./$JAVA_SERVICE/runserver $DEPLOY_FOLDER/$JAVA_SERVICE

JAVA_SERVICE="DISAgentService"
mkdir $DEPLOY_FOLDER/$JAVA_SERVICE
mkdir $DEPLOY_FOLDER/$JAVA_SERVICE/tmp
mkdir $DEPLOY_FOLDER/$JAVA_SERVICE/log
cp -r ./$JAVA_SERVICE/cmd $DEPLOY_FOLDER/$JAVA_SERVICE
cp -r ./$JAVA_SERVICE/conf $DEPLOY_FOLDER/$JAVA_SERVICE
cp -r ./$JAVA_SERVICE/dist $DEPLOY_FOLDER/$JAVA_SERVICE
cp ./$JAVA_SERVICE/runserver $DEPLOY_FOLDER/$JAVA_SERVICE

